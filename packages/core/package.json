    "@emotion/cache": "^11.11.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.3",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@veramo/core": "^6.0.0",
    "@veramo/did-manager": "^6.0.0",
    "@veramo/did-resolver": "^6.0.0",
