{
  "name": "@my-scope/core",
  "version": "0.0.1",
  "private": false,
  "description": "Core React components and hooks for the arr monorepo.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
